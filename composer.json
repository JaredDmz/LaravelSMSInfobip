{
  "name": "jareddmz/laravel-infobip-sms",
  "description": "Configurar y utilizar de forma sencilla el envío de SMS´s a través de Infobip.",
  "license": "MIT",
  "keywords": [
    "laravel",
    "sms",
    "infobip",
    "mensajes"
  ],
  "authors": [
    {
      "name": "Jared Dmz",
      "email": "jrd22dmz@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7"
  },
  "autoload": {
    "psr-4": {
      "JaredDmz\\LaravelInfobipSMS\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "JaredDmz\\LaravelInfobipSMS\\SMSIProvider"
      ],
      "aliases": {
        "SMSI": "JaredDmz\\LaravelInfobipSMS\\SMSIFacade"
      }
    }
  },
  "minimum-stability":"dev"
}
